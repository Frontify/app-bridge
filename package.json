{
    "name": "@frontify/app-bridge",
    "version": "2.12.0-beta.1",
    "description": "Package to establish communication between Frontify and marketplace apps",
    "module": "dist/index.js",
    "main": "dist/index.js",
    "files": [
        "dist"
    ],
    "types": "dist/index.d.ts",
    "scripts": {
        "clean": "rimraf dist",
        "test": "jest",
        "start": "webpack --config webpack.config.dev.js",
        "typecheck": "tsc --noEmit",
        "lint": "eslint ./src",
        "lint:fix": "eslint ./src --fix",
        "build": "npm run clean && npm run build:ts",
        "build:ts": "npx tsc",
        "prepare": "husky install"
    },
    "author": "Frontify Developers <developers@frontify.com>",
    "devDependencies": {
        "@frontify/eslint-config-react": "0.14.0",
        "@types/jest": "27.4.0",
        "@types/react": "17.0.38",
        "husky": "7.0.4",
        "jest": "27.4.7",
        "prettier": "2.5.1",
        "rimraf": "3.0.2",
        "ts-jest": "27.1.3",
        "typescript": "4.5.5"
    },
    "peerDependencies": {
        "react": "^17.0.2"
    },
    "engines": {
        "node": ">=16"
    }
}
