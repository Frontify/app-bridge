{
    "name": "@frontify/app-bridge",
    "version": "2.10.0",
    "description": "Package to establish communication between Frontify and marketplace apps",
    "module": "dist/index.js",
    "files": [
        "dist"
    ],
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        },
        "./react": {
            "import": "./dist/react/index.js",
            "require": "./dist/react/index.js"
        },
        "./utilities": {
            "import": "./dist/utilities/index.js",
            "require": "./dist/utilities/index.js"
        }
    },
    "scripts": {
        "clean": "rimraf dist",
        "test": "jest",
        "start": "webpack --config webpack.config.dev.js",
        "typecheck": "tsc --noEmit",
        "lint": "eslint ./src",
        "lint:fix": "eslint ./src --fix",
        "build": "npm run clean && npm run build:ts && npm run build:types",
        "build:ts": "npx babel --extensions .ts src/ -d dist",
        "build:types": "tsc --emitDeclarationOnly",
        "prepare": "husky install"
    },
    "author": "Frontify Developers <developers@frontify.com>",
    "devDependencies": {
        "@babel/cli": "7.16.0",
        "@babel/core": "7.16.0",
        "@babel/preset-env": "7.16.4",
        "@babel/preset-react": "^7.16.0",
        "@babel/preset-typescript": "7.16.0",
        "@frontify/eslint-config-react": "0.13.2",
        "@types/jest": "27.0.3",
        "@types/react": "17.0.37",
        "husky": "7.0.4",
        "jest": "27.4.3",
        "prettier": "2.5.1",
        "rimraf": "3.0.2",
        "ts-jest": "27.1.0",
        "typescript": "4.5.2"
    },
    "peerDependencies": {
        "react": "^17.0.2"
    },
    "engines": {
        "node": ">=16"
    },
    "dependencies": {
        "core-js": "^3.19.3"
    }
}
